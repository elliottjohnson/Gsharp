(in-package :gsharp-glyphs)

(defmacro defglyph (name)
  (let ((drawing-function (intern (concatenate 'string
					       "DRAW-"
					       (symbol-name name)))))
    `(defun ,name (&rest args)
      (let ((design (make-postscript-path)))
	(apply (function ,drawing-function) design args)
	design))))

