(defun draw-measure-rest (design)
  (comment design "measure rest")
  (moveto design 0 .25)
  (rlineto design 0 -.5)
  (moveto design .5 .25)
  (rlineto design 0 -.5)
  (draw design style)
  (setf (line-width design) .225)
  (moveto design 0 0)
  (lineto design .5 0)
  (draw design style)
  (setf (line-width design) 0))
